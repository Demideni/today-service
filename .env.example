DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB?sslmode=require
JWT_SECRET=replace_me_with_long_random_string

# S3-compatible storage (R2/AWS/Render Object Storage)
S3_ENDPOINT=https://YOUR-ENDPOINT
S3_REGION=auto
S3_BUCKET=your-bucket
S3_ACCESS_KEY_ID=xxx
S3_SECRET_ACCESS_KEY=xxx
S3_FORCE_PATH_STYLE=false

# Public base URL for served files
S3_PUBLIC_BASE_URL=https://PUBLIC-BASE-URL-TO-BUCKET
